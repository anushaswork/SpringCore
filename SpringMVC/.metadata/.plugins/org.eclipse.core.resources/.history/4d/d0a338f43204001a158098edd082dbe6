	@ResponseBody
	@RequestMapping("/giveMakeUp")
	public String getMakeUp()
	{
		return "Here is ur Makeup";
	}

	@ResponseBody
	@RequestMapping("/giveMakeUp")
	public String getMakeUp()
	{
		return "Here is ur Makeup";
	}
	
}
